                      <maml:para>Comment</maml:para>
                    </maml:headerEntry>
                  </maml:row>
                </maml:tableHeader>
                <maml:row>
                  <maml:entry>
                    <maml:para>Interval Seconds</maml:para>
                  </maml:entry>
                  <maml:entry>
                    <maml:para>3600</maml:para>
                  </maml:entry>
                  <maml:entry>
                    <maml:para>Frequency (in seconds) with which to run the check.</maml:para>
                  </maml:entry>
                </maml:row>
                <maml:row>
                  <maml:entry>
                    <maml:para>Unknown Error Detection Enabled</maml:para>
                  </maml:entry>
                  <maml:entry>
                    <maml:para>true</maml:para>
                  </maml:entry>
                  <maml:entry>
                    <maml:para>Enable unknown error detection.</maml:para>
                  </maml:entry>
                </maml:row>
